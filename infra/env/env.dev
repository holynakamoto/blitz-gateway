# Development environment variables
ENV=development
DEBUG=1
QUIC_LOG=debug

# Resource limits
CPU_LIMIT=2
CPU_RESERVATION=1
MEMORY_LIMIT=1G
MEMORY_RESERVATION=512M

# WASM Plugin settings
PLUGIN_DIR=/app/plugins
PLUGIN_MEMORY_LIMIT=2097152
PLUGIN_TIMEOUT=3000
PLUGIN_HOT_RELOAD=true

# Grafana password for development
GRAFANA_PASSWORD=dev_password